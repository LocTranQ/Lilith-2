<!--
CMS result for H -> mumu decay mode at mH = 125.38 GeV.
Best-fit values and uncertainties taken from Fig. 11a, correlation fitted from 95% CL in Fig. 11b (estimated) at  
http://cms-results.web.cern.ch/cms-results/public-results/publications/HIG-19-006/index.html
Implemented by William Vaginay, 22/04/2022
-->

<expmu decay="mumu" dim="4" type="vn">
  <experiment>CMS</experiment>
  <source type="published">HIG-19-006</source>
  <sqrts>13</sqrts>
  <mass>125.38</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="d1" prod="VBF">1.0</eff>
  <eff axis="d2" prod="ggH">1.0</eff>
  <eff axis="d3" prod="ttH">1.0</eff>
  <eff axis="d4" prod="VH">1.0</eff>

  <bestfit>
    <d1>1.36</d1>
	  <d2>0.63</d2>
	  <d3>2.32</d3>
	  <d4>5.48</d4>
  </bestfit>
 
  <param>
    <uncertainty axis="d1" side="left">-0.61</uncertainty>
    <uncertainty axis="d1" side="right">+0.69</uncertainty>
    <uncertainty axis="d2" side="left">-0.64</uncertainty>
    <uncertainty axis="d2" side="right">+0.65</uncertainty>
    <uncertainty axis="d3" side="left">-1.95</uncertainty>
    <uncertainty axis="d3" side="right">+2.27</uncertainty>
    <uncertainty axis="d4" side="left">-2.83</uncertainty>
    <uncertainty axis="d4" side="right">+3.10</uncertainty>
    
    <correlation entry="d1d2">-0.24</correlation>
    <correlation entry="d1d3">-0.24</correlation>
    <correlation entry="d1d4">0</correlation>
    
    <correlation entry="d2d3">0</correlation>
    <correlation entry="d2d4">-0.24</correlation>
    
    <correlation entry="d3d4">-0.24</correlation>
  </param>
</expmu>
