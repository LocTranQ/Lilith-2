<!--
CMS combination of the H -> ZZ* decay modes for 139/fb.
Best-fit values and uncertainties taken from Auxillary Figure 06, 
correlations from Aux Figure 07
http://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2021-23/
validated: waiting  
-->

<expmu dim="4" type="vn">
    <experiment>ATLAS</experiment>
    <source type="published">HIGG-2021-23</source>
    <sqrts>13</sqrts>
    <mass>125.09</mass>
    <CL>95%</CL> <!-- optional -->

    <eff axis="d1" prod="ggH" decay="ZZ">0.988636</eff>
    <eff axis="d1" prod="bbH" decay="ZZ">0.011364</eff>
    <eff axis="d2" prod="VBF" decay="ZZ">1.0</eff>
    <eff axis="d3" prod="VH" decay="ZZ">1.0</eff>
    <eff axis="d4" prod="top" decay="ZZ">1.0</eff>

    <bestfit>
        <d1>0.95</d1>
        <d2>1.33</d2>
	<d3>1.50</d3>
        <d4>1.68</d4>
    </bestfit>

    <param>
        <uncertainty axis="d1" side="left">-0.10</uncertainty>
        <uncertainty axis="d1" side="right">+0.11</uncertainty>
        <uncertainty axis="d2" side="left">-0.43</uncertainty>
        <uncertainty axis="d2" side="right">+0.52</uncertainty>
        <uncertainty axis="d3" side="left">-0.94</uncertainty>
        <uncertainty axis="d3" side="right">+1.17</uncertainty>
        <uncertainty axis="d4" side="left">-1.11</uncertainty>
        <uncertainty axis="d4" side="right">+1.68</uncertainty>

        <correlation entry="d1d2">-0.23</correlation>
        <correlation entry="d1d3">-0.26</correlation>
        <correlation entry="d1d4">0.02</correlation>
        
        <correlation entry="d2d3">-0.04</correlation>
        <correlation entry="d2d4">0.0</correlation>
        
        <correlation entry="d3d4">-0.18</correlation>
    </param>
</expmu>
