<!--
CMS combination of the H -> tautau decay modes for 139/fb.
Best-fit values and uncertainties taken from Auxillary Figure 06, 
correlations from Aux Figure 07
http://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2021-23/
validated: waiting  
-->

<expmu dim="4" type="vn">
    <experiment>ATLAS</experiment>
    <source type="published">HIGG-2021-23</source>
    <sqrts>13</sqrts>
    <mass>125.09</mass>
    <CL>95%</CL> <!-- optional -->

    <eff axis="d1" prod="ggH" decay="tautau">0.988636</eff>
    <eff axis="d1" prod="bbH" decay="tautau">0.011364</eff>
    <eff axis="d2" prod="VBF" decay="tautau">1.0</eff>
    <eff axis="d3" prod="VH" decay="tautau">1.0</eff>
    <eff axis="d4" prod="top" decay="tautau">1.0</eff>

    <bestfit>
        <d1>0.90</d1>
        <d2>1.00</d2>
	<d3>1.00</d3>
        <d4>1.37</d4>
    </bestfit>

    <param>
        <uncertainty axis="d1" side="left">-0.26</uncertainty>
        <uncertainty axis="d1" side="right">+0.29</uncertainty>
        <uncertainty axis="d2" side="left">-0.18</uncertainty>
        <uncertainty axis="d2" side="right">+0.21</uncertainty>
        <uncertainty axis="d3" side="left">-0.59</uncertainty>
        <uncertainty axis="d3" side="right">+0.62</uncertainty>
        <uncertainty axis="d4" side="left">-0.75</uncertainty>
        <uncertainty axis="d4" side="right">+0.86</uncertainty>

        <correlation entry="d1d2">-0.28</correlation>
        <correlation entry="d1d3">-0.33</correlation>
        <correlation entry="d1d4">-0.02</correlation>
        
        <correlation entry="d2d3">0.09</correlation>
        <correlation entry="d2d4">0.01</correlation>
        
        <correlation entry="d3d4">-0.01</correlation>
    </param>
</expmu>
