<!--
CMS combination of the H -> bb decay modes for 139/fb.
Best-fit values and uncertainties taken from Auxillary Figure 06, 
correlations from Aux Figure 07
http://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2021-23/
validated: waiting  
-->

<expmu dim="4" type="vn">
    <experiment>ATLAS</experiment>
    <source type="published">HIGG-2021-23</source>
    <sqrts>13</sqrts>
    <mass>125.09</mass>
    <CL>95%</CL> <!-- optional -->

    <eff axis="d1" prod="WH" decay="bb">1.0</eff>
    <eff axis="d2" prod="ZH" decay="bb">1.0</eff>
    <eff axis="d3" prod="top" decay="bb">1.0</eff>
    <eff axis="d4" prod="ggH" decay="bb">0.915789</eff>
    <eff axis="d4" prod="VBF" decay="bb">0.073684</eff>
    <eff axis="d4" prod="bbH" decay="bb">0.010526</eff>

    <bestfit>
        <d1>1.06</d1>
        <d2>1.00</d2>
	<d3>0.35</d3>
        <d4>0.98</d4>
    </bestfit>

    <param>
        <uncertainty axis="d1" side="left">-0.26</uncertainty>
        <uncertainty axis="d1" side="right">+0.28</uncertainty>
        <uncertainty axis="d2" side="left">-0.23</uncertainty>
        <uncertainty axis="d2" side="right">+0.25</uncertainty>
        <uncertainty axis="d3" side="left">-0.33</uncertainty>
        <uncertainty axis="d3" side="right">+0.34</uncertainty>
        <uncertainty axis="d4" side="left">-0.36</uncertainty>
        <uncertainty axis="d4" side="right">+0.38</uncertainty>

        <correlation entry="d1d2">0.01</correlation>
        <correlation entry="d1d3">0.01</correlation>
        <correlation entry="d1d4">0.0</correlation>
        
        <correlation entry="d2d3">0.0</correlation>
        <correlation entry="d2d4">0.0</correlation>
        
        <correlation entry="d3d4">0.0</correlation>
    </param>
</expmu>
