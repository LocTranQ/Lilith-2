<!--
CMS result for H -> ZZ decay mode at mH = 125.38 GeV for 137/fb.
Best-fit values and uncertainties taken from Table 5 (STXS), correlations from Fig. 15a at 
http://cms-results.web.cern.ch/cms-results/public-results/publications/HIG-19-001/
Comment: Table 4 with signal strengths has WH and ZH channels, but correlations available only for VH 
Validated: tbd
-->

<expmu dim="4" type="vn">
  <experiment>CMS</experiment>
  <source type="published">HIG-19-001</source>
  <sqrts>13</sqrts>
  <mass>125.38</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="d1" prod="ggH" decay="ZZ">1.0</eff>
  <eff axis="d2" prod="VBF" decay="ZZ">1.0</eff>
  <eff axis="d3" prod="VH" decay="ZZ">1.0</eff>
  <eff axis="d4" prod="ttH" decay="ZZ">1.0</eff>

  <bestfit>
  	<d1>1.02</d1>
	  <d2>0.50</d2>
	  <d3>1.56</d3>
	  <d4>0.16</d4>
  </bestfit>
 
  <param>
    <uncertainty axis="d1" side="left">-0.10</uncertainty>
    <uncertainty axis="d1" side="right">+0.11</uncertainty>
    <uncertainty axis="d2" side="left">-0.36</uncertainty>
    <uncertainty axis="d2" side="right">+0.44</uncertainty>
    <uncertainty axis="d3" side="left">-1.34</uncertainty>
    <uncertainty axis="d3" side="right">+1.99</uncertainty>
    <uncertainty axis="d4" side="left">-0.16</uncertainty>
    <uncertainty axis="d4" side="right">+0.98</uncertainty>
    
    <correlation entry="d1d2">-0.67</correlation>
    <correlation entry="d1d3">-0.12</correlation>
    <correlation entry="d1d4">0.02</correlation>
    
    <correlation entry="d2d3">0.06</correlation>
    <correlation entry="d2d4">-0.01</correlation>
    
    <correlation entry="d3d4">-0.06</correlation>
  </param>
</expmu>
