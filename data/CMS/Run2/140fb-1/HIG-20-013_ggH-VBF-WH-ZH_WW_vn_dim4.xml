<!--
CMS result for H -> WW decay mode at mH = 125.38 GeV.
Best-fit values and uncertainties taken from Fig.22, correlations from Fig. 23 at 
http://cms-results.web.cern.ch/cms-results/public-results/preliminary-results/HIG-20-013/index.html
Implemented by Axel Iohner, 2022/01/21
Validated: TBD
-->

<expmu dim="4" type="vn">
  <experiment>CMS</experiment>
  <source type="published">HIG-20-013</source>
  <sqrts>13</sqrts>
  <mass>125.38</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="d1" prod="ZH" decay="WW">1.0</eff>
  <eff axis="d2" prod="WH" decay="WW">1.0</eff>
  <eff axis="d3" prod="VBF" decay="WW">1.0</eff>
  <eff axis="d4" prod="ggH" decay="WW">1.0</eff>

  
  <bestfit>
  	<d1>2.0</d1>
	<d2>2.2</d2>
	<d3>0.71</d3>
	<d4>0.92</d4>
  </bestfit>
 
  <param>
    <uncertainty axis="d1" side="left">-0.7</uncertainty>
    <uncertainty axis="d1" side="right">+0.7</uncertainty>
    <uncertainty axis="d2" side="left">-0.6</uncertainty>
    <uncertainty axis="d2" side="right">+0.6</uncertainty>
    <uncertainty axis="d3" side="left">-0.25</uncertainty>
    <uncertainty axis="d3" side="right">+0.28</uncertainty>
    <uncertainty axis="d4" side="left">-0.10</uncertainty>
    <uncertainty axis="d4" side="right">+0.11</uncertainty>
  
        
    <correlation entry="d1d2">-0.04</correlation>
    <correlation entry="d1d3">0.00</correlation>
    <correlation entry="d1d4">0.00</correlation>
    
    <correlation entry="d2d3">0.00</correlation>
    <correlation entry="d2d4">-0.01</correlation>
    
    <correlation entry="d3d4">-0.13</correlation>
   
  </param>
</expmu>
