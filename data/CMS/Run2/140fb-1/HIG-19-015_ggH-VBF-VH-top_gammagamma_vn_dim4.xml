<!--
CMS result for H -> gammagamma decay mode at mH = 125.38 GeV.
Best-fit values and uncertainties taken from Fig. 16, correlations from Add. Fig. 3 at 
http://cms-results.web.cern.ch/cms-results/public-results/preliminary-results/HIG-19-015/
-->

<expmu dim="4" type="vn">
  <experiment>CMS</experiment>
  <source type="preliminary">HIG-19-015</source>
  <sqrts>13</sqrts>
  <mass>125.38</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="d1" prod="ggH" decay="gammagamma">1.0</eff>
  <eff axis="d2" prod="VBF" decay="gammagamma">1.0</eff>
  <eff axis="d3" prod="VH" decay="gammagamma">1.0</eff>
  <eff axis="d4" prod="top" decay="gammagamma">1.0</eff>

  <bestfit>
  	<d1>0.98</d1>
	<d2>1.15</d2>
	<d3>0.71</d3>
	<d4>1.40</d4>
  </bestfit>
 
  <param>
    <uncertainty axis="d1" side="left">-0.115</uncertainty>
    <uncertainty axis="d1" side="right">+0.13</uncertainty>
    <uncertainty axis="d2" side="left">-0.31</uncertainty>
    <uncertainty axis="d2" side="right">+0.36</uncertainty>
    <uncertainty axis="d3" side="left">-0.28</uncertainty>
    <uncertainty axis="d3" side="right">+0.31</uncertainty>
    <uncertainty axis="d4" side="left">-0.27</uncertainty>
    <uncertainty axis="d4" side="right">+0.33</uncertainty>
    
    <correlation entry="d1d2">-0.18</correlation>
    <correlation entry="d1d3">-0.03</correlation>
    <correlation entry="d1d4">0.04</correlation>
    
    <correlation entry="d2d3">0.02</correlation>
    <correlation entry="d2d4">0.03</correlation>
    
    <correlation entry="d3d4">-0.06</correlation>
  </param>
</expmu>
