<!--
ATLAS 139/fb result for mu(H->ZZ) at mH = 125.09 GeV for 4 prod. modes;
best fit-values and uncertainties fitted from Table 9, 
correlations taken from Fig. 12b in CONF-2019-02,
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/CONFNOTES/ATLAS-CONF-2019-025/
NB: SM uncertainties not taken into account in (sig*BR)/(sig*BR)SM !!
-->

<expmu dim="4" type="vn">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2016-21</source>
  <sqrts>13</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="d1" prod="ggH" decay="gammagamma">1.0</eff>
  <eff axis="d2" prod="VBF" decay="gammagamma">1.0</eff>
  <eff axis="d3" prod="VH" decay="gammagamma">1.0</eff>
  <eff axis="d4" prod="top" decay="gammagamma">1.0</eff>

  <bestfit>
    <d1>0.98</d1>
    <d2>1.4</d2>
    <d3>1.3</d3>
    <d4>1.2</d4>
  </bestfit>
 
  <param>
    <uncertainty axis="d1" side="left">-0.1086</uncertainty>
    <uncertainty axis="d1" side="right">+0.1086</uncertainty>
    <uncertainty axis="d2" side="left">-0.5196</uncertainty>
    <uncertainty axis="d2" side="right">+0.5196</uncertainty>
    <uncertainty axis="d3" side="left">-0.9274</uncertainty>
    <uncertainty axis="d3" side="right">+0.9274</uncertainty>
    <uncertainty axis="d4" side="left">-0.911</uncertainty>
    <uncertainty axis="d4" side="right">+1.4071</uncertainty>
     
    <correlation entry="d1d2">-0.22</correlation>
    <correlation entry="d1d3">-0.24</correlation>
    <correlation entry="d1d4">0.02</correlation>
    <correlation entry="d2d3">-0.03</correlation>
    <correlation entry="d2d4">0.0</correlation>
    <correlation entry="d3d4">-0.14</correlation>
  </param>
</expmu>
