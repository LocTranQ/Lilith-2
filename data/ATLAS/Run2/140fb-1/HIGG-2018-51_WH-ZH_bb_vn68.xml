<!--
ATLAS result for mu(WH, bb) & mu(ZH, bb) at mH = 125 GeV,
with best-fit values and uncertainties taken from Fig. 4,
and their correlation fitted from 95% CL in Add. Fig. 25, at
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2018-51/
-->

<expmu decay="bb" dim="2" type="vn">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2018-51</source>
  <sqrts>13</sqrts>
  <mass>125</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="WH">1.0</eff>
  <eff axis="y" prod="ZH">1.0</eff>

  <bestfit>
    <x>0.95</x>
    <y>1.08</y>
  </bestfit>
 
  <param>
    <uncertainty axis="x" side="left">-0.25</uncertainty>
    <uncertainty axis="x" side="right">+0.27</uncertainty>
    <uncertainty axis="y" side="left">-0.23</uncertainty>
    <uncertainty axis="y" side="right">+0.25</uncertainty>
    <correlation>0.022462961241534445</correlation>
  </param>
</expmu>